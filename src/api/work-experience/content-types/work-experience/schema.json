{
  "kind": "collectionType",
  "collectionName": "work_experiences",
  "info": {
    "singularName": "work-experience",
    "pluralName": "work-experiences",
    "displayName": "Work Experience"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "jobTitle": {
      "type": "string",
      "required": true
    },
    "companyName": {
      "type": "string",
      "required": true
    },
    "companyLogo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "endDate": {
      "type": "date",
      "required": false
    },
    "isCurrentJob": {
      "type": "boolean",
      "default": false
    },
    "descriptions": {
      "type": "json"
    },
    "order": {
      "type": "integer",
      "default": 0
    }
  }
}

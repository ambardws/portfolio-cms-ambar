{
  "kind": "collectionType",
  "collectionName": "tech_stacks",
  "info": {
    "singularName": "tech-stack",
    "pluralName": "tech-stacks",
    "displayName": "Tech Stack"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "category": {
      "type": "enumeration",
      "enum": ["frontend", "backend", "tools", "design", "database", "devops"],
      "default": "frontend"
    },
    "proficiencyLevel": {
      "type": "integer",
      "min": 1,
      "max": 100,
      "default": 50
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "featured": {
      "type": "boolean",
      "default": false
    }
  }
}
